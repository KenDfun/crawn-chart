
<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>HTML5サンプル</title>
  <link rel="stylesheet" type="text/css" href="style.css">
</head>
<body>
  <% @harrayCompanyInfo.each do |info| %>
    <% get_chartName(info) %>
    <div class="stock_chart">
      <div class="chart_png">
        <a href="<%= @fileName %>">
        <img class="alignnone size-full wp-image-8" src="<%= @fileName %>" alt="chart_<%= @strTickerCode %>" width="630" height="382"></a>
      </div>
      <div class="chart_article">
        <ul>
          <li><%= @name %></li>
          <li>終値:<%= @price %></li>
        </ul>
      </div>
    </div>
  <% end %>
</body>
</html>
